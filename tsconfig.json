{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "target": "esnext",
      "moduleResolution": "Node16",
      "esModuleInterop": true,
      "module": "Node16",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "baseUrl": "./cypress",
      "paths": {
        "@pages": ["support/pages/index.ts"],
        "@data": ["fixtures/users_data/index.ts"],
        "@api": ["support/api/index.ts"],
        "@utils": ["support/utils.ts"],
      }
    },
  }